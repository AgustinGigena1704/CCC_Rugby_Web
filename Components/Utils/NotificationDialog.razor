@using MudBlazor

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h6">@Titulo</MudText>
        <MudText>@Descripcion</MudText>
    </DialogContent>
</MudDialog>    

@code {
    [Parameter] public string Titulo { get; set; } = string.Empty;
    [Parameter] public string Descripcion { get; set; } = string.Empty;     
}
