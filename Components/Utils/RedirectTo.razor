@inject NavigationManager navManager

@if (ChildContent != null)
{
    @ChildContent
}

@code {
    [Parameter]
    public string Path { get; set; } = "/Auth/NotAuth";

    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    protected override void OnInitialized()
    {
        navManager.NavigateTo(Path);
    }
}
